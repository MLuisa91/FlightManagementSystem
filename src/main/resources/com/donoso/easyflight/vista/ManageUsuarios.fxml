<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" layoutX="8.0" layoutY="8.0" prefHeight="548.0" prefWidth="885.0"
            visible="false" fx:controller="com.donoso.easyflight.controlador.ManageUsuariosController">
    <children>
        <AnchorPane layoutX="7.0" layoutY="8.0" prefHeight="531.0" prefWidth="872.0"
                    stylesheets="@../estilos/GestionVuelos.css">
            <children>
                <AnchorPane prefHeight="302.0" prefWidth="871.0" stylesheets="@../estilos/GestionVuelos.css">
                    <styleClass>
                        <String fx:value="white-bg"/>
                        <String fx:value="shadow"/>
                    </styleClass>
                    <children>
                        <TableView fx:id="tableViewUsuarios" layoutX="32.0" layoutY="48.0" prefHeight="231.0"
                                   prefWidth="805.0" onMouseClicked="#handleRowSelection">
                            <columns>
                                <TableColumn fx:id="column_IdUsuario" prefWidth="45.600006103515625" text="ID"/>
                                <TableColumn fx:id="column_Nombre" prefWidth="136.79998779296875" text="Nombre"/>
                                <TableColumn fx:id="column_Apellidos" prefWidth="224.79998779296875" text="Apellidos"/>
                                <TableColumn fx:id="column_Pais" prefWidth="88.0" text="País"/>
                                <TableColumn fx:id="column_Email" prefWidth="183.20001220703125" text="Email"/>
                                <TableColumn fx:id="column_Telefono" prefWidth="128.0" text="Teléfono"/>
                            </columns>
                        </TableView>
                        <TextField fx:id="txtSearchUsuarios" layoutX="32.0" layoutY="14.0" prefHeight="26.0"
                                   prefWidth="283.0" promptText="Search" styleClass="search"
                                   stylesheets="@../estilos/GestionVuelos.css" onKeyReleased="#buscar"/>
                    </children>
                </AnchorPane>
                <TextField fx:id="txt_IdUsuario" layoutX="120.0" layoutY="323.0" prefHeight="26.0" prefWidth="175.0"
                           styleClass="textfield" stylesheets="@../estilos/GestionVuelos.css" promptText="00000000X"/>
                <Label layoutX="88.0" layoutY="329.0" text="ID:"/>
                <Label layoutX="82.0" layoutY="458.0" text="User:"/>
                <Label layoutX="54.0" layoutY="418.0" text="Apellidos:"/>
                <Label layoutX="58.0" layoutY="374.0" text="Nombre:"/>
                <TextField fx:id="txt_TelefonoUsuario" layoutX="424.0" layoutY="413.0" prefHeight="26.0"
                           prefWidth="175.0" styleClass="textfield" stylesheets="@../estilos/GestionVuelos.css"/>
                <TextField fx:id="txt_EmailUsuario" layoutX="424.0" layoutY="370.0" prefHeight="26.0" prefWidth="175.0"
                           styleClass="textfield" stylesheets="@../estilos/GestionVuelos.css"/>
                <Label layoutX="384.0" layoutY="329.0" text="País:"/>
                <Label layoutX="362.0" layoutY="418.0" text="Teléfono:"/>
                <Label layoutX="380.0" layoutY="374.0" text="Email:"/>
                <ComboBox fx:id="combo_PaisUsuairio" layoutX="423.0" layoutY="325.0" prefHeight="26.0"
                          prefWidth="177.0"/>
                <Button fx:id="buttont_AddUser" layoutX="660.0" layoutY="325.0" mnemonicParsing="false"
                        onAction="#addUsuario" prefHeight="26.0" prefWidth="90.0" styleClass="buttonAction"
                        stylesheets="@../estilos/GestionVuelos.css" text="Add"/>
                <Button fx:id="buttont_UpdateUser" layoutX="660.0" layoutY="370.0" mnemonicParsing="false"
                        onAction="#updateUsuario" prefHeight="26.0" prefWidth="90.0" styleClass="buttonAction"
                        stylesheets="@../estilos/GestionVuelos.css" text="Update"/>
                <Button fx:id="buttont_ClearUser" layoutX="660.0" layoutY="414.0" mnemonicParsing="false"
                        onAction="#cleanFields" prefHeight="26.0" prefWidth="90.0" styleClass="buttonAction"
                        stylesheets="@../estilos/GestionVuelos.css" text="Clear"/>
                <Button fx:id="buttont_DeleteUser" layoutX="660.0" layoutY="454.0" mnemonicParsing="false"
                        onAction="#deleteUsuario" prefHeight="26.0" prefWidth="90.0" styleClass="buttonAction"
                        stylesheets="@../estilos/GestionVuelos.css" text="Delete"/>
                <TextField fx:id="txt_ApellidosUsuario" layoutX="120.0" layoutY="413.0" prefHeight="26.0"
                           prefWidth="175.0" styleClass="textfield" stylesheets="@../estilos/GestionVuelos.css"/>
                <TextField fx:id="txt_NombreUSuario" layoutX="122.0" layoutY="370.0" prefHeight="26.0" prefWidth="175.0"
                           styleClass="textfield" stylesheets="@../estilos/GestionVuelos.css"/>
                <TextField fx:id="txt_User_Usuario" layoutX="122.0" layoutY="454.0" prefHeight="26.0"
                           prefWidth="175.0" styleClass="textfield" stylesheets="@../estilos/GestionVuelos.css"/>
                <Label layoutX="352.0" layoutY="458.0" text="Contraseña:"/>
                <PasswordField fx:id="txt_PasswordUsuario" layoutX="424.0" layoutY="455.0" prefHeight="26.0"
                               prefWidth="177.0" styleClass="textfield" stylesheets="@../estilos/GestionVuelos.css"/>
                <Label layoutX="85.0" layoutY="490.0" text="Rol:"/>
                <Group fx:id="checkGroup" layoutX="122.0" layoutY="490.0">
                    <children>
                        <CheckBox fx:id="check_Admin" mnemonicParsing="false" text="Admin" />
                        <CheckBox fx:id="check_Oferta" layoutX="102.0" mnemonicParsing="false" text="Oferta" />
                        <CheckBox fx:id="check_Reserva" layoutX="204.0" mnemonicParsing="false" text="Reserva" />
                        <CheckBox fx:id="check_Cliente" layoutX="301.0" mnemonicParsing="false" text="Cliente" />
                    </children>
                </Group>
            </children>
            <styleClass>
                <String fx:value="shadow"/>
                <String fx:value="white-bg"/>
            </styleClass>

        </AnchorPane>
    </children>
</AnchorPane>
